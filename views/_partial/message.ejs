<% if(typeof message !== 'undefined' && message) { %>
<div class="alert alert-info alert-dismissible" role="alert">
    <button type="button" class="close" onclick="this.parentElement.parentElement.removeChild(this.parentElement);"><span>&times;</span></button>
    <!-- 提示信息 -->
    <strong>信息</strong> <%- message %>
</div>
<% } %>